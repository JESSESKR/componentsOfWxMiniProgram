.numselectwrap {
  font-size: 0;
  white-space: nowrap;
  display: inline-block;
}
.numselectwrap .num-minus {
  background-color: #fff;
  border: 1px solid #999;
  width: 60rpx;
  height: 50rpx;
  line-height: 42rpx;
  text-align: center;
  color:#999;
  opacity: 0.5;
  font-size: 36rpx;
  box-sizing: border-box;
  border-top-left-radius: 8rpx;
  border-bottom-left-radius: 8rpx;
  display: inline-block;
}
.numselectwrap .num-minus.active {
  opacity: 1;
}
.numselectwrap .num-input {
  display: inline-block;
  width: 60rpx;
  height: 50rpx;
  vertical-align: top;
}
.numselectwrap .num-input .input {
  text-align: center;
  font-size: 24rpx;
  width: 100%;
  height: 50rpx;
  line-height: 30rpx;
  min-height: auto;
  background-color: #fff;
  border-top: 1px solid #999;
  border-bottom: 1px solid #999;
  box-sizing: border-box;
}
.numselectwrap .num-plus {
  background-color: #fff;
  border: 1px solid #999;
  width: 60rpx;
  height: 50rpx;
  line-height: 40rpx;
  text-align: center;
  color:#999;
  opacity: 0.5;
  font-size: 36rpx;
  box-sizing: border-box;
  border-top-right-radius: 8rpx;
  border-bottom-right-radius: 8rpx;
  display: inline-block;
  vertical-align: top;
}
.numselectwrap .num-plus.active {
  opacity: 1;
}



