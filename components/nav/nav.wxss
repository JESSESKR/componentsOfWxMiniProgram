.navwrap {
  position: relative;
  display: block;
}
.navwrap .navbar {
  position: fixed;
  min-height: 0px;
	padding-right: 220rpx;
	z-index: 9999;
  width: 100%;
	top: 0;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.navleft {
  display:flex;
  align-items:center;
  height:100%;
  justify-content:center;
  max-width:100%;
  font-size:30rpx;
  width: 80rpx;
}
.navback {
  display: inline-block;
  width: 16rpx;
  height: 16rpx;
  border-top: 2px solid #fff;
  border-left: 2px solid #fff;
  transform: rotate(-45deg);
}
.navbar .content {
  position:absolute;
  text-align:center;
  left:0;
  right:0;
  bottom:0;
  top:0;
  margin:auto;
  height:60rpx;
  font-size:32rpx;
  line-height:60rpx;
  cursor:none;
  pointer-events:none;
  text-overflow:ellipsis;
  white-space:nowrap;
  overflow:hidden;
  width: 200rpx;
}