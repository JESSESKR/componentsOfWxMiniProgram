.mask {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background-color: rgba(0, 0, 0, .7);
  z-index: 999;
  opacity: 0;
  transition: opacity .2s;
}
.panel {
  position: fixed;
  z-index: 1000;
  bottom: 0;
  left: 0;
  width: 100%;
  background-color: #fff;
  padding: 30rpx 0;
  transform: translate(0, 100%);
  opacity: 0;
  transition: all .2s;
}
.panel.padding-0 {
  padding: 0;
}
.panel.nobg {
  background-color: transparent;
}
.mask.on {
  opacity: 1;
}
.panel.on {
  opacity: 1;
  transform: translate(0, 0);
}
.modal-close {
  position: absolute;
  top: -90rpx;
  right: 20rpx;
  display: inline-block;
  width: 70rpx;
  height: 70rpx;
  background-repeat: no-repeat;
	background-size: 100% 100%;
  background-image: url('data:image/png;base64,');
}
